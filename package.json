{
  "name": "dem-bs-server",
  "version": "0.1.0",
  "description": "Example Reason Esy Project",
  "license": "MIT",
  "esy": {
    "build": "dune build -p #{self.name}",
    "buildDev": "refmterr dune build --promote-install-files --root . --only-package #{self.name}",
    "NOTE": "Optional release Section. Customizes result of `esy release`",
    "release": {
      "bin": [
        "Main"
      ]
    },
    "buildEnv": {
      "ODOC_SYNTAX": "re"
    }
  },
  "scripts": {
    "start": "esy x Main",
    "dev": "esy dune build --watch",
    "format": "esy dune build @fmt --auto-promote",
    "doc": "esy dune build @doc"
  },
  "dependencies": {
    "ocaml": "~4.10",
    "@esy-ocaml/reason": "~3.6.0",
    "@opam/dune": "~2.6.2",
    "@opam/opium": "~0.18.0",
    "@reason-native/console": "~0.1.0",
    "@reason-native/pastel": "~0.3.0",
    "@enspirit/dem-bs": "0.15.3"
  },
  "devDependencies": {
    "@opam/ocaml-lsp-server": "ocaml/ocaml-lsp:ocaml-lsp-server.opam",
    "@reason-native/rely": "~3.2.1",
    "refmterr": "~3.3.2",
    "ocaml": "~4.10",
    "@opam/odoc": "*"
  }
}
